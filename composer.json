{
    "name": "kozennnn/chronopost-api",
	"minimum-stability": "stable",
    "description": "A PHP package to make the Chronopost API easier to use",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathis HOUIS",
            "email": "mathishouis@gmail.com"
        }
    ],
    "require": {
		"php": "^7.3|^8.0",
        "ext-soap": "*",
      "ext-curl": "*",
        "ext-json": "*"
    },
	"require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
	"autoload": {
        "psr-4": {
            "Kozennnn\\ChronopostAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kozennnn\\ChronopostAPI\\Tests\\": "tests/"
        }
    }
}
